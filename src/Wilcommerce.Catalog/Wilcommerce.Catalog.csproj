<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <VersionPrefix>1.0.0-beta4</VersionPrefix>
    <Authors>Alberto Mori</Authors>
    <TargetFramework>netstandard1.6</TargetFramework>
    <AssemblyName>Wilcommerce.Catalog</AssemblyName>
    <PackageId>Wilcommerce.Catalog</PackageId>
    <PackageTags>wilcommerce;ecommerce;opensource;.netcore;.net;catalog</PackageTags>
    <PackageIconUrl>http://www.wilcommerce.com/logo64.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/wilcommerce/Wilcommerce.Catalog</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/wilcommerce/Wilcommerce.Catalog/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/wilcommerce/Wilcommerce.Catalog</RepositoryUrl>
    <NetStandardImplicitPackageVersion>1.6.1</NetStandardImplicitPackageVersion>
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Linq.Queryable" Version="4.3.0" />
    <PackageReference Include="Wilcommerce.Core.Common" Version="1.0.0-beta4" />
    <PackageReference Include="Wilcommerce.Core.Infrastructure" Version="1.0.0-beta4" />
  </ItemGroup>

</Project>
